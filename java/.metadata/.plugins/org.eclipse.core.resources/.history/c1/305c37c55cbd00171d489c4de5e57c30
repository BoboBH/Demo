package com.bobo.mybatis.bean;


import java.util.List;
import org.apache.ibatis.annotations.Select;
import com.bobo.mybatis.bean.UserMassage;;
public interface UserMessageMapper {

	@Select("select * from user_message where id=#{id}")
	public  List<UserMassage> getUserMessageById(int id);
	
	@Insert("insert into user_message(id, message) values(#{id}, '#{message})'")
	public void save(UserMessage message);
}
